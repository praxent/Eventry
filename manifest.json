{
    "name": "Eventry Plugin",
    "version": "1.0",
    "description": "Log your hours in one click!",
    "manifest_version": 2,
    "background": {
    "scripts": ["js/background.js"],
    "persistent": false
    },
    "browser_action": {
      "default_popup": "pages/index.html"
    },
     "permissions": ["storage"],
     "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'"

  }